    <% results.forEach(row => { %>  
        <tr>
            <td> <%= row.first_name %> </td>,
            <td> <%= row.last_name %> </td>,
            <td> <%= row.provider_speciality %> </td>,
        </tr>
    <% }); %>
  </tbody>

</table>